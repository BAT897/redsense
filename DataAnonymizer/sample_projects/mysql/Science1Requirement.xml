<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Requirement>
    <Client>Science1</Client>
    <Version>1.0</Version>
    <Tables>
        <Table Name="applications" PKey="id">
            <Columns>
                <Column Name="first_name" ReturnType="String" IgnoreEmpty="true">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomFirstNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="last_name" ReturnType="String" IgnoreEmpty="true">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomLastNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="middle_name" ReturnType="String" IgnoreEmpty="true">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomMiddleNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="preferred_str" ReturnType="String" IgnoreEmpty="true">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomPreferredName.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="gender" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="M|F"/>
					</Parameters>
                </Column>
                <Column Name="city" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomCities.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="province" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomProvinces.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="country" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomCountries.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="postal_code" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
                    <Parameters>
                        <Parameter Name="regex" Value="[0-9]{5}|[A-Z][0-9][A-Z] [0-9][A-Z][0-9]"/>
                    </Parameters>
                </Column>
                <Column Name="phone" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="\|\|\||[0-9]{3}\|[0-9]{3}\|[0-9]{4}\||[1-9]{1,3}\|[0-9]{3}\|[0-9]{4}\|[0-9]{1,2}|[1-9]{2}\|[0-9]{2}\|[0-9]{4}\|[0-9]{4}"/>
					</Parameters>
                </Column>
                <Column Name="fax" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="\|\|\||[0-9]{3}\|[0-9]{3}\|[0-9]{4}\||[1-9]{1,3}\|[0-9]{3}\|[0-9]{4}\|[0-9]{1,2}|[1-9]{2}\|[0-9]{2}\|[0-9]{4}\|[0-9]{4}"/>
					</Parameters>
                </Column>
                <Column Name="email" ReturnType="String" IgnoreEmpty="true">
					<!-- randomEmail doesn't guarantee uniqueness - neither does this, but less likely to clash given number of possibilities -->
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="[0-9A-Za-z]{10}\@unreal\.ubc\.ca"/>
					</Parameters>
                </Column>
                <Column Name="student_no" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="[1-9][0-9]{7}"/>
					</Parameters>
                </Column>
                <Column Name="ubc_registration_number" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="[1-9][0-9]{7}"/>
					</Parameters>
                </Column>
                <Column Name="degree" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomDegrees.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="hs_name" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="3" Type="String"/>
						<Parameter Name="length" Value="30" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="hs_city" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomCities.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="hs_province" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomProvinces.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="hs_country" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomStringFromFile</Function>
                    <Parameters>
                        <Parameter Name="file" Value="RandomCountries.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="hs_phone" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="\|\|\||[0-9]{3}\|[0-9]{3}\|[0-9]{4}\||[1-9]{1,3}\|[0-9]{3}\|[0-9]{4}\|[0-9]{1,2}|[1-9]{2}\|[0-9]{2}\|[0-9]{4}\|[0-9]{4}"/>
					</Parameters>
                </Column>
                <Column Name="hs_fax" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="\|\|\||[0-9]{3}\|[0-9]{3}\|[0-9]{4}\||[1-9]{1,3}\|[0-9]{3}\|[0-9]{4}\|[0-9]{1,2}|[1-9]{2}\|[0-9]{2}\|[0-9]{4}\|[0-9]{4}"/>
					</Parameters>
                </Column>
                <Column Name="street1" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="3" Type="String"/>
						<Parameter Name="length" Value="30" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="street1" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="3" Type="String"/>
						<Parameter Name="length" Value="30" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="street2" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="\|\|\||[0-9]{3}\|[0-9]{3}\|[0-9]{4}\|"/>
					</Parameters>
                </Column>
                <Column Name="date_birth" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="19[6-9][0-9]-(0[1-9]|1[0-2])-(0[1-9]|1[0-9]|2[0-8])"/>
					</Parameters>
                </Column>
                <Column Name="essay" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="300" Type="String"/>
						<Parameter Name="length" Value="4000" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="interest" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="5" Type="String"/>
						<Parameter Name="length" Value="80" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="website" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.generateStringFromPattern</Function>
					<Parameters>
						<Parameter Name="regex" Value="[0-9a-z]{6}\.unreal\.ubc\.ca"/>
					</Parameters>
                </Column>
                <Column Name="description" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="20" Type="String"/>
						<Parameter Name="length" Value="200" Type="String"/>
					</Parameters>
                </Column>
            </Columns>
        </Table>
        <Table Name="activities" PKey="id">
			<Columns>
				<Column Name="name" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="5" Type="String"/>
						<Parameter Name="length" Value="50" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="location" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="3" Type="String"/>
						<Parameter Name="length" Value="40" Type="String"/>
					</Parameters>
                </Column>
			</Columns>
		</Table>
		<Table Name="contests" PKey="id">
			<Columns>
				<Column Name="name" ReturnType="String" IgnoreEmpty="true">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="5" Type="String"/>
						<Parameter Name="length" Value="50" Type="String"/>
					</Parameters>
                </Column>
			</Columns>
		</Table>
		<Table Name="lacking_prereqs" PKey="id">
			<Columns>
				<Column Name="option_text" ReturnType="String">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="7" Type="String"/>
						<Parameter Name="length" Value="70" Type="String"/>
					</Parameters>
                </Column>
			</Columns>
		</Table>
		<Table Name="emails" PKey="id">
			<Columns>
				<Column Name="subject" ReturnType="String">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="4" Type="String"/>
						<Parameter Name="length" Value="50" Type="String"/>
					</Parameters>
                </Column>
                <Column Name="message" ReturnType="String">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="100" Type="String"/>
						<Parameter Name="length" Value="1000" Type="String"/>
					</Parameters>
                </Column>
			</Columns>
		</Table>
		<Table Name="reviews" PKey="date">
			<Columns>
				<Column Name="message" ReturnType="String">
					<Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
					<Parameters>
						<Parameter Name="num" Value="100" Type="String"/>
						<Parameter Name="length" Value="1000" Type="String"/>
					</Parameters>
                </Column>
			</Columns>
		</Table>
    </Tables>
</Requirement>
