<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Requirement>
    <Client>Integrated Science</Client>
    <Version>1.0</Version>
    <Tables>
        <Table Name="isp_students" PKey="id">
            <Columns>
                <Column Name="first" ReturnType="String">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomFirstName</Function>
                    <Parameters>
                        <Parameter Name="file" Value="/Users/sdi/work/ubc/demos/demo/RandomFirstNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="last" ReturnType="String">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomLastName</Function>
                    <Parameters>
                        <Parameter Name="file" Value="/Users/sdi/work/ubc/demos/demo/RandomLastNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="email" ReturnType="String">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomEmail</Function>
                    <Parameters>
                        <Parameter Name="domainName" Value="ubc.ca" Type="String"/>
                    </Parameters>
                    <Exception>test@test.domain.com</Exception>
                </Column>                
                <Column Name="description" ReturnType="String">
                    <Function>com.strider.dataanonymizer.functions.CoreFunctions.randomDescription</Function>
                    <Parameters>
                        <Parameter Name="min" Value="1" Type="String"/>
                        <Parameter Name="max" Value="100" Type="String"/>                        
                    </Parameters>
                </Column>
                <Column Name="student_no" ReturnType="String">
                    <Function>com.strider.dataanonymizer.extensions.IntegratedScience.randomStudentNumber</Function>
                </Column>                
            </Columns>
        </Table>
    </Tables>
</Requirement>
