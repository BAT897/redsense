<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Requirement>
    <Client>Integrated Science</Client>
    <Version>1.0</Version>
    <Tables>
        <Table Name="test" PKey="id">
            <Columns>
                <Column Name="first" ReturnType="String">
                    <Function>randomFirstName</Function>
                    <Parameters>
                        <Parameter Name="file" Value="/Users/strider/RandomFirstNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="last" ReturnType="String">
                    <Function>randomLastName</Function>
                    <Parameters>
                        <Parameter Name="file" Value="/Users/strider/RandomLastNames.txt" Type="String"/>
                    </Parameters>
                </Column>
                <Column Name="email" ReturnType="String">
                    <Function>randomEmail</Function>
                    <Parameters>
                        <Parameter Name="domainName" Value="ubc.ca" Type="String"/>
                    </Parameters>
                </Column>                
                <Column Name="description" ReturnType="String">
                    <Function>randomDescription</Function>
                    <Parameters>
                        <Parameter Name="range" Value="1,100" Type="Range"/>
                    </Parameters>
                </Column>
                <Column Name="stud_no" ReturnType="String">
                    <Function>randomStudentNumber</Function>
                </Column>                
            </Columns>
        </Table>
    </Tables>
</Requirement>